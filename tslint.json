{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      [
        "img",
        "ionic-image-loader"
      ],
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      [
        "img",
        "ionic-image-loader"
      ],
      "kebab-case"
    ],
    "no-bitwise": false
  }
}
